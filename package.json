{
  "name": "nettopalkka-verolaskuri",
  "version": "1.0.0",
  "description": "Calculadora de salario neto e impuestos para Finlandia",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "scrape:municipal": "ts-node scripts/data-updater/scrapeMunicipalRates.ts",
