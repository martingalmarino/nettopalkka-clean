  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.5.8",
    "@types/node": "20.14.10",
    "@types/pdf-parse": "^1.1.4",
    "@types/react": "18.3.3",
    "@types/react-dom": "18.3.0",
    "autoprefixer": "10.4.19",
    "axios": "^1.6.0",
    "cheerio": "^1.0.0-rc.12",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^30.2.0",
